<% include partials/header.ejs %>
<% include partials/navbar2.ejs %>

<main role="main">
	<div class="container">

		<section class="jumbotron pb-0">
   			<h1 class="display-4 mt-4">DEEP Brewing Company</h1>
		</section>

       	<!-- Row 1 -->
        <div class="row">

		    <!-- Column 1 -->
		    <div class="col-6 mt-4"
				<h1 class="display-4">DEEP Brewing Company</h1>
				<hr>
			</div>
			<!-- End Column 1 -->

			<!-- Column 2 -->
			<div class="col-6 mt-4"
				<div class="mapouter">
					<div class="img-fluid gmap_canvas">
						<iframe width="500" height="400" id="gmap_canvas" src="https://maps.google.com/maps?q=deep%20brewing%20company%20tallahassee&t=&z=13&ie=UTF8&iwloc=&output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe>
					</div>
				</div>
			</div>
			<!-- End Column 2 -->

		</div>
		<!-- End Row 1 -->


        <!-- Row 2 -->
        <div class="row justify-content-center pt-5">

            <!-- Carousel -->
            <div id="carouselBeer" class="carousel slide" data-ride="carousel">

                <ol class="carousel-indicators">
                    <li data-target="#carouselIndicators" data-slide-to="0" class="active"></li>
                    <li data-target="#carouselIndicators" data-slide-to="1"></li>
                    <li data-target="#carouselIndicators" data-slide-to="2"></li>
                </ol>

                <div class="carousel-inner" role="listbox">
                    <div class="carousel-item active">
                        <img class="d-block w-100" src="images/deep1.jpg" alt="First slide">
                    </div>

                    <div class="carousel-item">
                        <img class="d-block w-100" src="images/deep2.jpg" alt="Second slide">
                    </div>

                    <div class="carousel-item">
                        <img class="d-block w-100" src="images/deep3.jpg" alt="Third slide">
                    </div>
                </div>

                <a class="carousel-control-prev" href="#carouselBeer" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span></a>

                <a class="carousel-control-next" href="#carouselBeer" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span></a>

            </div>
            <!-- End Carousel -->

        </div>
        <!-- End Row 2 -->

	</div>
	<!-- End Container -->
</main>

<% include partials/footer.ejs %>
